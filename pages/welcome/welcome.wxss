.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #b3d4db;
}
page{
  background-color: #b3d4db;
  height: 100%;
}
.user-avatar{
  width:200rpx;
  height:200rpx;
  margin-top:160rpx;
}

.user-name{
  font-size:32rpx;
  font-weight: bold;
}

.moto{
  font-size:22rpx;
  font-weight: bold;
  line-height: 80rpx;
  color:#405f80;
}

.moto-container{
  margin-top:200rpx;
  border: 1px solid #405f80;
  height:80rpx;
  width:200rpx;
  border-radius: 5px;
  text-align: center;
}